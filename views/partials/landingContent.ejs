  <!-- Product Box -->

  <% for(i=0; i>products.length; i++) {%>
  <div class="col-md-3">
    <div class="card h-100">
        <img class="card-img-top" src="<%=product.img%>" alt="" id="experiencePrimaryImage">
        <div class="card-body">
          <h4 class="card-title" id="experienceTittle"><%product.name%></h4>
          <ul class="list-inline small how-many-stars stars-left-align">
              <li class="list-inline-item m-0"><i class="far fa-star text-gray"></i></li>
              <li class="list-inline-item m-0"><i class="fas fa-star text-success"></i></li>
              <li class="list-inline-item m-0"><i class="fas fa-star text-success"></i></li>
              <li class="list-inline-item m-0"><i class="fas fa-star text-success"></i></li>
              <li class="list-inline-item m-0"><i class="fas fa-star text-success"></i></li>
          </ul>
          <span class="text-muted font-weight-normal font-italic d-block small" id="categoryName">Categoría: <%product.category%></span>
          <p class="card-text" id="experienceDescrption"><%product.description%></p>
          <a href="/products/<%product.id%>" class="btn btn-primary">Quiero saber más!</a>
        </div>
      </div>
</div>

<% } %>
  <!-- /Page Content -->